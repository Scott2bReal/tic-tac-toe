<div class='grid'>
  <div class="cell">Cell 1</div> <div class="cell">Cell 2</div> <div class="cell">Cell 3</div>
  <div class="cell">Cell 4</div> <div class="cell">Cell 5</div> <div class="cell">Cell 6</div>
  <div class="cell">Cell 7</div> <div class="cell">Cell 8</div> <div class="cell">Cell 9</div>
</div>

<form method='post' action='/play'>
  <div class='grid'>
    <% @board.squares.each do |num, marker| %>
      <div class="cell" id='square-<%= num %>'>
        <button type='submit'><%= marker %></button>
      </div>
    <% end %>
  </div>
</form>
